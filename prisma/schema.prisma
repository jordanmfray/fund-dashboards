generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int    @id @default(autoincrement())
  email String @unique
  name  String
}

model Organization {
  Id                Int     @id @default(autoincrement())
  Ein               String  @unique
  Name              String
  WebsiteUrl        String? @unique
  GuideStarURL      String? @unique
  NteeCode          String?
  NteeDescription   String?
  BusinessZip       String?
  RegionServed      String?
  About             String?
  OrgImages         String?
  LogoURL           String?
  Tags              String?
}